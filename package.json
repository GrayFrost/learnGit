{
  "name": "learngit",
  "version": "1.0.0",
  "description": "Learn how to use git",
  "main": "index.js",
  "dependencies": {
    "commitizen": "^4.0.3",
    "custom-git-commit": "file:.yalc/custom-git-commit"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "commitlint-config-cz": "^0.12.1",
    "cz-conventional-changelog": "^3.0.2",
    "cz-customizable": "^6.2.0",
    "husky": "^3.0.9"
  },
  "scripts": {
    "test": "git-mkt",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrayFrost/learnGit.git"
  },
  "author": "gary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GrayFrost/learnGit/issues"
  },
  "homepage": "https://github.com/GrayFrost/learnGit#readme"
}
